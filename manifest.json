{
  "name": "Searchy Start",
  "description": "Open multiple search engines with one query",
  "version": "0.7",
  "permissions": [
    "contextMenus",
    "storage",
    "downloads",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icons/search128r.png"
  },
  "icons": {
    "128": "icons/search128r.png"
  },
  "omnibox": {
    "keyword": "sy"
  },
  "options_page": "options/options/options.html",
  "manifest_version": 3
}
